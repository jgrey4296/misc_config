# -*- mode: snippet -*-
# name: doom.filetemplates
# uuid: doom.filetemplates
# key:  doom.filetemplates
# group :
# --
;;-- file templates
(after! jg-file-templates
  ;; $1
  (+jg-completion-add-file-templates
   '${1:mode}
   '(
     ("$1.$2$"           :trigger "__" :mode $1-mode)
     )
   )
)
;;-- end file templates
