# -*- mode: snippet -*-
# name:  alphabet-char-insert-template
# key: alpha-insert-template
# condition: t
# --
(map! :map $1
      :prefix ($2 . $3)
      :i "a"  (insert ?\" (char-from-name "LATIN SMALL LETTER A WITH CEDILLA") ?\")
      :i "b"  (insert ?\" (char-from-name "LATIN SMALL LETTER B WITH CEDILLA") ?\")
