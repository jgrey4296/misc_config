      :i "l"  (insert ?\" (char-from-name "LATIN SMALL LETTER L WITH CEDILLA") ?\")
      :i "m"  (insert ?\" (char-from-name "LATIN SMALL LETTER M WITH CEDILLA") ?\")
      :i "n"  (insert ?\" (char-from-name "LATIN SMALL LETTER N WITH CEDILLA") ?\")
