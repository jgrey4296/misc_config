import { Router, Link from 'preact-router';

render(){
    return (
        <Router onChange={ (e)=>{this.currentUrl = e;}; }>
        <SomePage path="/" />
        </Router>
    )
};


renderHead(){
    return (
        <nav>
        <Link href="/">
        </nav>
    );
}