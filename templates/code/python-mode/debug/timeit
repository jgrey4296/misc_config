import timeit
import time
# see https://docs.python.org/3/library/timeit.html
def autorange_cb(number, took):
    logging.info("Number: {:>3}, Took: {:8.2} seconds", number, time)


timer = timeit.Timer({$1:'test(2, 200)'}, setup={$2:'print("setting up")'}, globals=globals())

timer.autorange(autorange_cb)
timer.timeit(number=10)
timer.repeat(repeat=5, number=10)
